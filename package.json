{
  "name": "blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npx tailwindcss -i ./assets/css/main.css -o ./assets/_gen/style.css",
    "watch": "npx tailwindcss -i ./assets/css/main.css -o ./assets/_gen/style.css --watch",
    "format": "prettier --write './assets/**/*.{css,js}' './{content,layouts}/**/*.html'"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "prettier": "^3.3.2",
    "prettier-plugin-go-template": "^0.0.15"
  }
}
